<!DOCTYPE html> 
<html>
  <head>
    #parse( "azkaban/webapp/servlet/velocity/style.vm" )
    <script type="text/javascript" src="${context}/static/js/jquery/jquery.js"></script>    
    <script type="text/javascript" src="${context}/static/js/namespace.js"></script>
    <script type="text/javascript" src="${context}/static/js/underscore-1.2.1-min.js"></script>
    <script type="text/javascript" src="${context}/static/js/backbone-0.5.3-min.js"></script>
    <script type="text/javascript" src="${context}/static/js/azkaban.nav.js"></script>
  </head>
  <body>
    #set($current_page="all")
    #parse( "azkaban/webapp/servlet/velocity/nav.vm" )
    <div class="content">
      <div id="all-jobs-content">
        <div class="section-hd">
          <h2>All Jobs</h2>
      
          <form method="get">
            <input type="hidden" name="action" value="search">
            <input type="submit" value="Search" class="search-btn">
            <input type="text" placeholder="by job name or wildcard" value="" class="search-input" name="job">
          </form>

          <a class="btn1 upload" href="#">Upload Job</a>
        </div><!-- end .section-hd -->
      </div>
    <div class="content">
  </body>
